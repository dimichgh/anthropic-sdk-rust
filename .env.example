# Anthropic SDK Configuration
# Copy this file to .env and fill in your actual values

# Standard Anthropic API
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_BASE_URL=https://api.anthropic.com

# Custom Gateway Configuration
# For custom API gateways that use Bearer token authentication
CUSTOM_BEARER_TOKEN=your-custom-bearer-token-here
CUSTOM_BASE_URL=https://your-custom-gateway.example.com/v1/anthropic

# Model Configuration
# Use your custom gateway's specific model names
CUSTOM_MODEL_NAME=claude-3-5-sonnet-latest
# Example for custom gateways that have specific model names:
# CUSTOM_MODEL_NAME=hubgpt-chat-completions-sonnet-3-7
